

article, aside, details, figcaption, figure, footer, header, hgroup, main, nav, section, summary {
    display: block;
}

.list-group .list-group-item:hover h5 a {
    transition: color 0.3s ease;
}

a:hover {
    color: #FF800A;
    transition: color 0.3s ease;
     text-decoration: none !important;
}

a:hover, a:focus {
    color: #181a1c;
    text-decoration: none;
}

a:hover, a:focus {
    color: #2a6496;
    text-decoration: none !important;
}

a:active, a:hover {
    outline: 0;
}

 

body a:hover {
    text-decoration: none !important;
}
